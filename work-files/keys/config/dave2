echo "duplicate_threads";   toggle duplicate_threads
echo "reply_regex";         set    reply_regex         = "other"
echo "sort";                set    sort                = "date"
echo "sort_aux";            set    sort_aux            = "from"
echo "sort_re";             toggle sort_re
echo "strict_threads";      toggle strict_threads
echo "thread_received";     toggle thread_received
echo "use_threads";         set    use_threads         = "yes"

